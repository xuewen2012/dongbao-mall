<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuewen.dongbao.ums.mapper.UmsMemberMapper">
    <select id="selectByName" parameterType="String" resultType="com.xuewen.dongbao.ums.entity.UmsMember">
        select * from ums_member where username = #{name} limit 1
    </select>

</mapper>
